<div class="container">
<h2><%= @photo.title %></h2>

<%= image_tag @photo.image_url.to_s, class: "img-responsive"%>

<small>Uploaded by <%= @photo.user.email %>
<span class="timestamp"><%= time_ago_in_words(@photo.created_at) %> ago.</span></small>

<p><%= link_to "back", photos_path, class: "btn btn-default" %>
<%= link_to "delete", photo_path(@photo), method: :delete, class: "btn btn-default" %>
</p>
</div>